{
    "julia.additionalArgs": [
        "-t 8,4"
    ],
    "julia.enableTelemetry": true,
    "files.exclude": {
        "**/.git/**.jl": true,
        "**/.julia*": true,
        "**/*.dSYM": true,
        },
    //"vscode-neovim.neovimPath": "nvim",
    "editor.scrollBeyondLastLine": false,
    "editor.rulers": [
        92
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": ["K"],
            "after": [
                "i",
                "<CR>",
                "<Escape>",
                "k",
                "$",
            ]
        },
        {
            "before": ["s"],
            "commands": [
                ":w"
            ]
        },
        {
           "before": ["u"],
           "commands": [
               "undo"
           ]
        },
        {
           "before": ["<C-r>"],
           "commands": [
               "redo"
           ]
        },
        {
           "before": ["g","f"],
           "commands": [
               "editor.action.openLink"
           ]
        }
    ],
    "vim.textwidth": 92,
    "julia.enableCrashReporter": true,
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.scrollback": 100000,
    "terminal.integrated.shellArgs.osx": "zsh",
    "window.autoDetectHighContrast": false,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "editor.accessibilitySupport": "off",
    "julia.symbolCacheDownload": true,
    "files.insertFinalNewline": true,
    "vim.commandLineModeKeyBindings": [




    ],
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace, 'Liberation Mono for Powerline'",
    "githubPullRequests.terminalLinksHandler": "github",
    "go.toolsManagement.autoUpdate": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "githubPullRequests.pullBranch": "never",
    "redhat.telemetry.enabled": true,
    "files.associations": {
        "*.jl": "julia"
    },
    "julia.executablePath": "julia",
    "editor.inlineSuggest.enabled": true,
    "git.autofetch": true,
    "explorer.confirmDragAndDrop": false,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "git.openRepositoryInParentFolders": "never",
    "zig.path": "",
    "zig.zls.path": "/Users/nathandaly/Library/Application Support/Code/User/globalStorage/ziglang.vscode-zig/zls_install/zls",
    "zig.initialSetupDone": true,
    "jupyter.askForKernelRestart": false,
    "github.copilot.editor.enableAutoCompletions": true,
    "jupyter.interactiveWindow.creationMode": "perFile",
    "github.copilot.nextEditSuggestions.enabled": true,
    "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "claudeCode.preferredLocation": "panel",
}

